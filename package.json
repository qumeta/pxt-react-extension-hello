{
  "name": "pxt-react-extension-hello",
  "version": "0.0.1",
  "scripts": {
    "build:dev": "./node_modules/.bin/webpack --mode development --config webpack.dev.js",
    "build:prod": "./node_modules/.bin/webpack --mode production --config webpack.prod.js",
    "start": "npm run build:dev && webpack-dev-server --open --config webpack.dev.js",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "autoprefixer": "9.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "eventemitter3": "3.1.0",
    "express": "^4.17.3",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "semantic-ui-css": "2.4.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.1",
    "tslint": "5.11.0",
    "tslint-microsoft-contrib": "5.2.1",
    "typescript": "^3.1.6",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "semantic-ui-react": "^0.87.3"
  }
}
